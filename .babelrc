{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": false,
                    "node": true
                },
                "useBuiltIns": "usage",
                "debug": false
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime"],
        ["transform-async-to-module-method"],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}
